{
    "name": "digitalingenieur/contao-churchtools",
    "type": "contao-bundle",
    "description": "Contao 4 Churchtools bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Samuel Heer",
            "homepage": "https://github.com/digitalingenieur"
        }
    ],
    "require": {
        "contao/core-bundle": "~3.5 || ~4.1",
        "contao-community-alliance/composer-plugin": "~2.4 || ~3.0",
        "digitalingenieur/churchtools-php-sdk": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Diging\\ChurchtoolsBundle\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Diging\\ChurchtoolsBundle\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "contao-manager-plugin": "Diging\\ChurchtoolsBundle\\ContaoManager\\Plugin"
    }
}
